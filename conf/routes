GET     /timeout/:duration                      controllers.TimeoutController.timeout(duration: Long)

GET     /circuit-breaker                        controllers.CircuitBreakerController.circuitBreaker()

GET     /retry                                  controllers.RetryController.retry()

GET     /delay                                  controllers.DelayController.delay(sleep: Long, delay: Long)

GET     /assets/*file                           controllers.Assets.versioned(path="/public", file: Asset)
